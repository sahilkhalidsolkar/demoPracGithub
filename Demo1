// branching and merging
-------------------------------------------------------------------------------------------
docker 

docker images // lists all the images

docker info // gives info about the dockcer

alpine // linux os image

docker pull alpine // import alpine image

docker run alpine sh  // isse run hoti image lekin direct exit hoti
docker ps -a // checks weather container is runing or not

docker run -d -it alpine sh // isse container create hota aur chalu rehte jabtak destroy nay hota tabh tak

docker run -it alpine sh // to enter into the alpine version ie into the linux terminal

docker run -it --name=myLinux alpine sh //to change the name of the container

docker stop (conatiner_name) eg docker stop myLinux // to stop the container

docker rm myLinux // to remove the container

docker rmi -f alpine // to remove the image
